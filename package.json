    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "codecov": "^3.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^2.0.0",
    "full-icu": "^1.3.1",
    "git-revision-webpack-plugin": "^3.0.6",
