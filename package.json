    "jszip": "^3.0.0",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.21",
    "loose-envify": "^1.4.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "nconf": "^0.11.0",
    "object-assign": "^4.1.1",
    "oidc-client": "^1.10.1",
    "progress": "^1.1.8",
    "prop-types": "^15.7.2",
    "pug": "^3.0.2",
    "raven-js": "^2.3.0",
    "react": "^16.12.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-bootstrap": "^1.4.3",
